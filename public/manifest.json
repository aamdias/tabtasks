{
    "manifest_version":3,
    "version":"1.0.0",
    "name":"ToDo Extension",
    "action":{
        "default_popup":"index.html"
    },
    "permissions":[
        "scripting",
        "activeTab"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "checklist-icon.png",
        "32": "checklist-icon.png",
        "48": "checklist-icon.png",
        "128": "checklist-icon.png"
    }
}